generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ari_users {
  id            Int       @id @default(autoincrement())
  id_base       Int
  iduser        Int
  email         String    @unique(map: "email") @db.VarChar(255)
  password      String    @db.VarChar(255)
  name          String    @db.VarChar(255)
  role          String?   @default("user") @db.VarChar(50)
  permissions   Json?
  last_access   DateTime? @db.Timestamp(0)
  access_count  Int?      @default(0)
  refresh_token String?   @db.VarChar(500)
  preferences   Json?
  theme         String?   @default("light") @db.VarChar(20)
  language      String?   @default("pt-BR") @db.VarChar(10)
  active        Boolean?  @default(true)
  created_at    DateTime? @default(now()) @db.Timestamp(0)
  updated_at    DateTime? @default(now()) @db.Timestamp(0)

  @@unique([id_base, iduser], map: "uk_base_user")
  @@index([active], map: "idx_active")
  @@index([email], map: "idx_email")
  @@index([last_access], map: "idx_last_access")
  @@index([role], map: "idx_role")
}

